{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Start Django Containers",
      "type": "shell",
      "command": "docker-compose",
      "args": [
        "-f",
        "docker-compose.debug.yml",
        "--project-name",
        "django-debugging",
        "up",
        "-d"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
